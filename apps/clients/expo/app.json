{
  "expo": {
    "name": "vento",
    "slug": "vento",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Used to report the device location to Vento",
        "NSCameraUsageDescription": "Used to control the flashlight",
        "NSMicrophoneUsageDescription": "Used to measure ambient sound levels",
        "NSBluetoothAlwaysUsageDescription": "Used to scan and connect to Bluetooth devices",
        "NSBluetoothPeripheralUsageDescription": "Used to communicate with Bluetooth peripherals"
      },
      "bundleIdentifier": "com.protofy.vento"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "INTERNET",
        "RECORD_AUDIO",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_ADVERTISE",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.protofy.vento"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d2c51664-47e8-4b42-9c7f-71dc3b682298"
      }
    }
  }
}
